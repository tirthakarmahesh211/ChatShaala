<style>
  /* --------------- */
/*Navbar*/


  ._topnav {
    overflow: hidden;
    background-color: #333;
    position: relative;
  }

  ._topnav #myLinks {
    display: none;
  }

  /* Style navigation menu links */
  ._topnav a {
    color: white;
    padding: 14px 16px;
    text-decoration: none;
    font-size: 17px;
    display: block;
    background-color: #333;
    text-align: center;
  }

  /* Style the hamburger menu */
  ._topnav a._icon {
    background: #333;
    float: left;
    display: block;
  }

  /* Add a grey background color on mouse-over */
  ._topnav a:hover {
    background-color: #ddd;
    color: black;
  }

  /* Style the active link (or home/logo) */
  ._topnav a._active {
    background-color: #376A97;
    color: white;
    text-align: center;
  }

  ._topnav img._logo {
    width: 1rem;

  }
</style>

